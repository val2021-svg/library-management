<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library2.BookloanController">
   <children>
      <AnchorPane layoutY="24.0" prefHeight="303.0" prefWidth="300.0" style="-fx-border-color: #ffffff;">
         <children>
            <TextField fx:id="titletxt" layoutX="29.0" layoutY="65.0" prefHeight="26.0" prefWidth="241.0" promptText="Book title*" />
            <TextField fx:id="keyword1txt" layoutX="29.0" layoutY="105.0" prefHeight="26.0" prefWidth="241.0" promptText="Keyword 1*" />
            <TextField fx:id="keyword2txt" layoutX="29.0" layoutY="145.0" prefHeight="26.0" prefWidth="241.0" promptText="Keyword 2" />
            <TextField fx:id="keyword3txt" layoutX="29.0" layoutY="185.0" prefHeight="26.0" prefWidth="241.0" promptText="Keyword 3" />
            <TextField fx:id="keyword4txt" layoutX="29.0" layoutY="225.0" prefHeight="26.0" prefWidth="241.0" promptText="Keyword 4" />
            <TextField fx:id="keyword5txt" layoutX="29.0" layoutY="265.0" prefHeight="26.0" prefWidth="241.0" promptText="Keyword 5" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="300.0" layoutY="65.0" prefHeight="50.0" prefWidth="300.0" style="-fx-border-color: #ffffff;">
         <children>
            <TextField fx:id="authornametxt" layoutX="24.0" layoutY="25.0" prefHeight="26.0" prefWidth="241.0" promptText="Author's name*" />
            <TextField fx:id="authorlastnametxt" layoutX="24.0" layoutY="65.0" prefHeight="26.0" prefWidth="241.0" promptText="Author's last name*" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="76.0" prefWidth="600.0" style="-fx-background-color: #89cff0;">
         <children>
            <Label fx:id="bookloanLabel" alignment="CENTER" prefHeight="76.0" prefWidth="600.0" text="Book Loan" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="43.0" />
               </font>
            </Label>
            <Button fx:id="logoutButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#logout" prefHeight="74.0" prefWidth="68.0" style="-fx-background-color: #89cff0;" text="&lt;&lt;" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Button>
            <Button fx:id="returnButton" alignment="CENTER" contentDisplay="CENTER" layoutX="68.0" mnemonicParsing="false" onAction="#returning" prefHeight="74.0" prefWidth="68.0" style="-fx-background-color: #89cff0;" text="&lt;" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Button>
            <Label fx:id="nameLabel2" alignment="CENTER" contentDisplay="CENTER" layoutX="449.0" layoutY="8.0" prefHeight="60.0" prefWidth="141.0" style="-fx-border-color: #ffffff;" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="300.0" layoutY="200.0" prefHeight="147.0" prefWidth="300.0">
         <children>
            <TextField fx:id="isbntxt" layoutX="24.0" layoutY="24.0" prefHeight="26.0" prefWidth="241.0" promptText="ISBN*" />
            <Label fx:id="editionLabel" layoutX="24.0" layoutY="65.0" prefHeight="58.0" prefWidth="242.0" textFill="#2ab44a">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="searchButton" layoutX="325.0" layoutY="278.0" mnemonicParsing="false" onAction="#search" prefHeight="38.0" prefWidth="115.0" stylesheets="@hello.css" text="Search">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label fx:id="leftredLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="329.0" prefHeight="67.0" prefWidth="242.0" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="rightgreenLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="322.0" layoutY="329.0" prefHeight="67.0" prefWidth="242.0" textAlignment="CENTER" textFill="#008000">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="loanButton" layoutX="456.0" layoutY="278.0" mnemonicParsing="false" onAction="#loan" prefHeight="38.0" prefWidth="109.0" stylesheets="@hello.css" text="Loan">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="leftgreenLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="329.0" prefHeight="67.0" prefWidth="242.0" textFill="GREEN">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="rightredLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="322.0" layoutY="329.0" prefHeight="67.0" prefWidth="242.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
